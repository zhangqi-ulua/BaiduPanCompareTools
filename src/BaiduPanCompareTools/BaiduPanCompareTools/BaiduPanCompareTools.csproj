<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <BaseOutputPath>..\..\..\release\</BaseOutputPath>
    <Version>1.1.0</Version>
    <Authors>张齐</Authors>
    <Company>张齐</Company>
    <Product>百度网盘对比工具</Product>
    <Copyright>张齐</Copyright>
    <AssemblyVersion>1.1.0</AssemblyVersion>
    <FileVersion>1.1.0</FileVersion>
    <Description>百度网盘对比工具</Description>
    <PackageProjectUrl>https://github.com/zhangqi-ulua</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="SnapshootAndLocalDirDiffResultForm.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="SnapshootViewerForm.cs">
      <SubType>Form</SubType>
    </Compile>
  </ItemGroup>

</Project>